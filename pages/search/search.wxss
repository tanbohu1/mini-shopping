.search-wrap{display:flex;height:100rpx;padding:10rpx 40rpx;font-size:30rpx;justify-content:center;align-items:center;background-color:#fcfafa;position:fixed;top:0;left:0;width:100%}.search-wrap .input-content{border:1px solid #797575;flex:2;height:70rpx;background-color:#fff}.search-wrap .mini-btn{color:var(--themeColor);margin:0 5rpx;border:0}.list-wrap{display:flex;flex-direction:column;align-items:stretch;margin-top:120rpx;margin-bottom:30rpx;background-color:#fcfafa;font-size:35rpx;color:#182457}.list-wrap .list-item{height:60rpx;line-height:60rpx;border-bottom:1px solid gray;padding:0 40rpx;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:1;-webkit-box-orient:vertical}